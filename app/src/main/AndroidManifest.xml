<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.connectall">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ConnectAll">
        <activity android:name=".Alumni_List"></activity>
        <activity android:name=".TalkActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".StudentProfileActivity" />
        <activity android:name=".LauncherActivity" />
        <activity android:name=".UploadResources" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.ConnectAll.NoActionBar" />
        <activity android:name=".AlumniMainProfile">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>